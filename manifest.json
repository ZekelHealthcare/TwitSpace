{
  "name": "TwitSpace",
  "version": "1.0",
  "description": "Bring a bit of that ol' Myspace magic to any tweet that mentions a band/artist name.",
  "browser_action": {
    "default_icon": "icon_ON.png", 
    "default_title": "Activate TwitSpace"
  },
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "css": ["twitspace.css"],
      "js": ["jquery-1.6.1.min.js","twitspace.js"]
    }
  ]
}